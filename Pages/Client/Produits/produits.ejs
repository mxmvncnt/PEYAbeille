<head>
  <%- include('./style.ejs'); %>
</head>

<%- include ('../../../Global/headerBar.ejs') %>

  <div class="">
    <div class="">
      <h1>Produits</h1>
      <% if (items !==undefined && items.length> 0) {%>
        <div class="grid-produits">
          <% items.forEach(function(item){ %>
            <div class="produit">
              <h2 class="titre"><%= item.NOM %></h2>
              <h3 class="prix"><%= item.PRIX_SUGGERE %> $</h3>
              <button><a href="<%= item.URL_CATALOG %>">Acheter</a></button>
            </div>
            <%}); %>
        </div>
        <% }else {%>
          <p><strong> Non disponible </strong></p>
        <% } %>
    </div>
  </div>
  <br />